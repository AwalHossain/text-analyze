{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@common/(.*)$": "<rootDir>/../src/common/$1",
    "^@utils/(.*)$": "<rootDir>/../src/common/utils/$1",
    "^src/(.*)$": "<rootDir>/../src/$1"
  },
  "setupFiles": ["<rootDir>/jest-e2e.setup.ts"]
}